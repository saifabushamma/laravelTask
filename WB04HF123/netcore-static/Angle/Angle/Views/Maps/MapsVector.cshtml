@{
    ViewBag.Title = "MapsVector";
}
<div class="content-heading">
   <div>Vector Maps
      <small>Resolution independent maps</small>
   </div>
</div>
<div data-vector-map="" data-height="700" data-map-name="world_mill_en"></div>

@section Styles {
    <environment names="Development">
        <link href="~/vendor/ika.jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" />
    </environment>
    <environment names="Staging,Production">
        <link href="~/vendor/ika.jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" />
    </environment>
}

@section Scripts {

    <environment names="Development">
        <script src="~/vendor/ika.jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
        <script src="~/vendor/ika.jvectormap/jquery-jvectormap-world-mill-en.js"></script>
        <script src="~/vendor/ika.jvectormap/jquery-jvectormap-us-mill-en.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/vendor/ika.jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
        <script src="~/vendor/ika.jvectormap/jquery-jvectormap-world-mill-en.js"></script>
        <script src="~/vendor/ika.jvectormap/jquery-jvectormap-us-mill-en.js"></script>
    </environment>

}

